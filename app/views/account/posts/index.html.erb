<div class="col-md-12">

  <h2 class="text-center"> My Published Posts </h2>

  <table class="table">
    <thead>
      <tr>
        <th> Content </th>
        <th> Group Name </th>
        <th> Last Update </th>
        <th colspan="2"></th>
      </tr>
    </thead>

    <tbody>

      <%= render :partial => "post_item", :collection => @posts, :as => :post %>
    </tbody>
  </table>
</div>
